{"config.json":"{\"mode\":\"block\",\"github\":null,\"board\":\"kidbright32-v1.6\",\"level\":\"Beginner\"}","extension":{"ioxhop_PMS7003-extension":{".gitattributes":"# Auto detect text files and perform LF normalization\n* text=auto\n","blocks":{"blocks.js":"Blockly.defineBlocksWithJsonArray([\n{\n  \"type\": \"pms7003_read\",\n  \"message0\": \"PMS7003 tx pin %1 read %2\",\n  \"args0\": [\n    {\n      \"type\": \"field_dropdown\",\n      \"name\": \"pin\",\n      \"options\": [\n        [\n          \"IN1\",\n          \"32\"\n        ],\n        [\n          \"IN2\",\n          \"33\"\n        ],\n        [\n          \"IN3\",\n          \"34\"\n        ],\n        [\n          \"IN4\",\n          \"35\"\n        ],\n        [\n          \"OUT1\",\n          \"26\"\n        ],\n        [\n          \"OUT2\",\n          \"27\"\n        ],\n        [\n          \"23\",\n          \"23\"\n        ],\n        [\n          \"19\",\n          \"19\"\n        ],\n        [\n          \"18\",\n          \"18\"\n        ]\n      ]\n    },\n    {\n      \"type\": \"field_dropdown\",\n      \"name\": \"type\",\n      \"options\": [\n        [\n          \"PM1.0\",\n          \"0\"\n        ],\n        [\n          \"PM2.5\",\n          \"1\"\n        ],\n        [\n          \"PM10.0\",\n          \"2\"\n        ]\n      ]\n    }\n  ],\n  \"inputsInline\": true,\n  \"output\": null,\n  \"colour\": \"#E67E22\",\n  \"tooltip\": \"Read particle concentration from PMS7003\",\n  \"helpUrl\": \"https://www.ioxhop.com/p/869\"\n}\n]);\n","generators.js":"Blockly.Python['pms7003_read'] = function(block) {\n  Blockly.Python.definitions_['import_pms7003'] = 'import pms7003';\n\n  var dropdown_clk = block.getFieldValue('pin');\n  var dropdown_type = block.getFieldValue('type');\n\n  var code = `pms7003.read(${dropdown_clk})[${dropdown_type}]`;\n  return [code, Blockly.Python.ORDER_NONE];\n};\n"},"extension.js":"({\n    name: \"PMS7003\", // Category Name\n    description: \"Digital universal particle concentration sensor\",\n    author: \"IOXhop.com\",\n    category: \"Sensors\",\n    version: \"1.0.0\",\n    icon: \"/static/icon.png\", // Category icon\n    color: \"#E67E22\", // Category color (recommend some blocks color)\n    blocks: [ // Blocks in Category\n        {\n            xml: `\n                <block type=\"pms7003_read\">\n                    <field name=\"pin\">32</field>\n                    <field name=\"type\">1</field>\n                </block>\n            `\n        }\n    ]\n});\n","modules":{"pms7003.py":"# Dev by Sonthaya Nongncuh, microBlock IDE\nfrom machine import UART\nfrom time import sleep_ms, ticks_ms\n\nuart = UART(2, 9600)\n\n__old_value = [ 0 ] * 3\n__pin = None\n__last_check = 0\n\ndef b2i(x, y):\n    return (x << 8 | y)\n\ndef read(pin):\n    global __old_value, __pin, __last_check\n\n    if pin != __pin:\n        uart.init(9600, bits=8, parity=None, stop=1, rx=pin, tx=-1)\n        __pin = pin\n        __last_check = 0\n\n    if (ticks_ms() - __last_check) < 100:\n        return __old_value\n\n    errCount = 0\n    while errCount < 30:\n        sleep_ms(100)\n        errCount = errCount + 1\n\n        packet = uart.read()\n        if not packet:\n            continue\n\n        errCount = 0\n        if len(packet) < 32:\n            print(\"Packet size vaild\")\n            continue\n\n        if packet[0:2] != b\"\\x42\\x4d\":\n            print(\"Start error\")\n            continue\n\n        packetSum = 0\n        for index in range(30):\n            packetSum += packet[index]\n        packetSum = packetSum & 0xFFFF\n        if packetSum != b2i(packet[30], packet[31]):\n            print(\"Checksum error\")\n            continue\n\n        for index in range(3):\n            __old_value[index] = b2i(packet[(index * 2) + 0 + 10], packet[(index * 2) + 1 + 10])\n\n        break\n    \n    __last_check = ticks_ms()\n    if errCount >= 30:\n        print(\"PMS7003 not connect ?\")\n        __old_value = [ -99, -99, -99 ]\n    return __old_value\n\n"},"static":{"icon.png":"data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAIAAAACACAYAAADDPmHLAAAABHNCSVQICAgI\nfAhkiAAAAAlwSFlzAAADsQAAA7EB9YPtSQAAABl0RVh0U29mdHdhcmUAd3d3\nLmlua3NjYXBlLm9yZ5vuPBoAABTjSURBVHic7Z15eFTV2cB/M5N9JQtbCFvC\nGhCFgEUBhaKiAoKA1KWfilSlWmvrUmop7trWrbRPPx8+awO2VfFhD4uCoEKA\ngCQENSFEkpCEbCQhGTLZJ5n7/XGZMHf2zNw7k2V+z3OfJ/fcc+95c847Z33P\ne8CHDx8+fPjw0RdRufHufOD/gCEyydJT0AMNgBaoA/KAs0AOcBio9p5onuUC\nIPguyWUAvgfeAaa4nrU9A29ndk+4fgB+C4S5mMfdGm9nbk+6aoAXgX4u5bSC\nuNMHEExv/rb8RjdF6Rl0GARa2zto1rejbWqjStdMZX0zhTX1lNY1YhAEe69X\nAc8D/8Es/7yFTwFkpFnfQd5FLSeLqsmtrKPDYLOMDwMPA+c9JpwNfAqgEA2t\neo7kV3LoXAVNbe3WomiBR4DtnpVMisaNd182vbljwlD3JOllBPhpGDUgklmj\nBuGv0VB8SUeHtHkIApYDwcBBrwgJqL2VcF8h0E/DvKR4Xrh9MhPioswfq4DV\nwEbAz9OygU8BPEZ0aCCPzRzPsikj8VNbZPuDwFa8oATu9AEA+NvyG1uBABlk\n6TOU1DbwzyNnqW9pM3/0EbACD44Q5KgBGmT4Rp9iWHQYv5k7kf5hQeaPHgLe\n9KQsbiuASqBCDkH6GjGhQTz902uItVSC1cBiT8nhtgK0dBgKii7pqNI1yyFP\nnyI8yJ8nbk4iIsjfNFgFpAAjPCGDO8PAxcCmA2dLZ6UXVqla2w1MGhItl1x9\nhpAAP0b1j+TboipMRonBwHXAv5VO350aoA64VhDEb5ypqHM0DerDBsOiw1h0\n7Qjz4NnA/Uqn7Y4CHAMuG28aWvWcr9G5L1Ef5abRg0kabDFP8A4QoWS67jQB\nBmAyMMEY0NCmJ3lYf7eF6ouogJGx4RwrvGhak4YBjUCaUum6owAAFxHHrQDU\n6FpIGhxFZLBvWsAVQgL8MCCQX1VvGjwJeB/REkl23B0FpAHpxhsB2PV9Mb6u\ngOvMHh1HSIBkQjAWeFSp9NytAUC0gbvPeHOpsRV/PzUJsYo2Xb0WP40afYeB\n/GpJLRAHrFciPTlmAlOBnaYBe7NLfB1CN5g5ahAatWSWfhLisFB25KgBAA4h\n9gWCAQQBvi+rZcyASF9/wAUC/TQU1zZQrWsxDa4HDsidllwK0IBo3bKMKwtM\n+g4DWRdqSOwfQVRIoEzJ9B3UKhWnSy+ZBoUgmuHLilwKAHAG0fjxTmNAu0Eg\no7gagwCJ/SNQqdxefOwz9AsJ4Ku8ctNlwYGIo4EmOdORUwEATiJausw0BggC\n5FfXc67qMkOjwogI8jUJzuCvUZNdXme6ZKwCjiNuQpENuRUAxCnixy0Cm9o4\nVnCRoks6IoMDiA4Nct8YoZdzUddE0SXJans2okGpbChhgTLT1gMByK3Uklup\nJTzInzEDIhk7sB8DwoMJD/InIsifAD8ldLJnMiA8xDxorNxpKKEAM0xv+gUH\noG22sHxB16Ins6SGzJIaBUTovkSFBLI8OcHavL8FA8MtbAXGyC2PEjaBEiGX\nTR7Jgz8ZTVykhTb3SeqaWtlwLI8ybaPDuFGhFqMn2RdalFCAGNObqJBAEmLC\neXTGOFZMH8N18TEEaPq2LWpbh4GUY3k0663uF+gkyLI5DJdbFiWagFjTm8Ar\n/4QKGB4dxvDoMOZPGErZ5SaKa3WUX26ivlmPrlVPQ6si6x3dkpqGF